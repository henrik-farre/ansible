{
  extends: [
    'config:recommended',
    'group:allNonMajor',
    ':disableRateLimiting',
  ],
  "timezone": "Europe/Copenhagen",
  "schedule": "every weekend",
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/^inventory/group_vars/docker\\.yaml$/',
      ],
      matchStrings: [
        'repo: (?<depName>.+)\\s+tag: (?<currentValue>.+)',
      ],
      datasourceTemplate: 'docker',
    },
  ],
  packageRules: [
    {
      matchCategories: [
        'docker',
        'custom',
      ],
      enabled: true,
    },
    {
      "versioning": "regex:^v?(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)((\\.|-r)(?<build>\\d+))?-ls(?<revision>\\d+)$",
      "matchPackageNames": [
        "/^(gh|ls)cr\\.io\\/linuxserver\\//"
      ]
    },
  ],
}
