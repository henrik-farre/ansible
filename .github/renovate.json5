{
  extends: [
    'config:recommended',
  ],
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/^inventory/group_vars/docker\\.yaml$/',
      ],
      matchStrings: [
        'repo: "(?<depName>[^"]+)"\\s+tag: "(?<currentValue>[^"]+)"',
      ],
      datasourceTemplate: 'docker',
    },
  ],
  packageRules: [
    {
      matchCategories: [
        'docker',
      ],
      enabled: true,
    },
  ],
}
