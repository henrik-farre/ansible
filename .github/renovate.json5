{
  extends: [
    'config:recommended',
    'schedule:weekly',
    'group:allNonMajor',
  ],
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/^inventory/group_vars/docker\\.yaml$/',
      ],
      matchStrings: [
        'repo: (?<depName>.+)\\s+tag: (?<currentValue>.+)',
      ],
      datasourceTemplate: 'docker',
    },
  ],
  packageRules: [
    {
      matchCategories: [
        'docker',
        'custom',
      ],
      enabled: true,
    },
  ],
}
